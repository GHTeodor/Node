<!doctype html>
<html lang="uk">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="./users_all.css">
    <title>Errors</title>
</head>
<body>
<header>
    <a href="/login">Login</a>
    <a href="/">API</a>
    <a href="/users">Users</a>
    <a href="/singIn">SingIn</a>
</header>

<h1>ERRORS</h1>
<p style="color: orangered">User with this Email is already exist</p>
{{#each errors}}
    <div id="user">
        <div>|{{this.firstName}}</div>
        <div>|{{this.lastName}}</div>
        <div>|{{this.email}}</div>
        <div>|{{this.password}}</div>
        <div>|{{this.age}}</div>
        <div>|{{this.city}}</div>
    </div>
{{/each}}
</body>
</html>