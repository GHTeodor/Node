<!doctype html>
<html lang="uk">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="Users_all.css">
    <title>User</title>
</head>
<body>
<header>
    <a href="/login">Login</a>
    <a href="/">API</a>
    <a href="/users">Users</a>
    <a href="/error">Errors</a>
</header>

<h1>USER</h1>
<div id="user">
    <ul style="list-style-type: square; background-color: aquamarine">
        {{#user}}
            <li>|{{this.firstName}}</li>
            <li>|{{this.lastName}}</li>
            <li>|{{this.email}}</li>
            <li>|{{this.password}}</li>
            <li>|{{this.age}}</li>
            <li>|{{this.city}}</li>
        {{/user}}
    </ul>
</div>
</body>
</html>